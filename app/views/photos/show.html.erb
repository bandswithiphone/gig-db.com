<div class="container-fluid">
  <div id="nav-bar">
    <div>
      <i class="large material-icons">queue_music</i>
      <h3 id ="gig-db">GIG-db.com</h3>
    </div>
    <div id='logout'>
      <% if current_user.present? %>
          <%= link_to 'Log out', logout_path %>
      <% else %>
          <%= link_to 'Login', login_path %>
      <% end %>
    </div>
  </div>
</div>

<br>
<div>
<% if current_user.present? %>
  <%= link_to 'Back to my concert list', user_concerts_path(current_user) %>
<% else %>
  <%= link_to 'Back to the main concert page', concerts_path %> |
  <%= link_to 'Create an account', new_user_path %>
<% end %>
</div>
<br>
<% @photos.each do |photo| %>
  <div class="instagram">
  <%= photo.photo.html_safe %> <%# VERY UNSAFE!!!!!! photo code comes from the user %>
  </div>
<% end %>


