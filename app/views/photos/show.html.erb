<div id="nav-bar">
  <div>
    <i class="large material-icons">queue_music</i>
    <h3 id ="gig-db">GIG-db.com</h3>
  </div>
  <div id='logout'>
    <% if current_user.present? %>
        <%= link_to 'Log out', logout_path %>
    <% else %>
        <%= link_to 'Login', login_path %>
    <% end %>
  </div>
</div>

<% @photos.each do |photo| %>
  <div class="instagram">
  <%= photo.photo.html_safe %> <%# VERY UNSAFE!!!!!! photo code comes from the user %>
  </div>
<% end %>

<%= link_to 'Back to my concert list', user_concerts_path(current_user) %>
